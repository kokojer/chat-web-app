{
  "name": "chat-web-app",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "config": {
    "port": "5000"
  },
  "prisma": {
    "schema": "apps/backend/prisma/schema.prisma",
    "seed": "apps/backend/prisma/seed.ts"
  },
  "scripts": {
    "all": "concurrently \"npm run dev\" \"npm run server\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "server": "json-graphql-server db.cjs --p %npm_package_config_port%",
    "db:create": "ts-node --esm apps/backend/prisma/scripts/createDatabase.ts",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node --esm apps/backend/prisma/scripts/seed.ts"
  },
  "author": "kokojer",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@apollo/client": "^3.8.5",
    "@prisma/client": "^5.4.2",
    "antd": "^5.9.4",
    "chalk": "^5.3.0",
    "graphql": "^14.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^3.4.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "json-graphql-server": "^2.3.2",
    "prisma": "^5.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
